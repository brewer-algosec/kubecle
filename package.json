{
  "name": "kubecle",
  "version": "1.0.0",
  "description": "Useful ui for kubernettes",
  "repository": "https://github.com/rydogs/kubecle",
  "main": "index.js",
  "engineStrict": true,
  "engines": {
    "node": ">=8.0.0",
    "npm": ">=5.6.0"
  },
  "scripts": {
    "start": "NODE_ENV=production node server/index",
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch": "NODE_ENV=development nodemon server/index",
    "client-build": "npm --prefix client run build && git add client/dist/bundle.js",
    "client-test": "npm --prefix client run test"
  },
  "pre-commit": [
    "client-test",
    "client-build"
  ],
  "keywords": [
    "kubernettes",
    "k8s"
  ],
  "author": "Ryan Liu",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.18.3",
    "express": "^4.16.3",
    "express-async-handler": "^1.1.3",
    "json-beautify": "^1.0.1",
    "kubernetes-client": "^5.3.1",
    "request": "^2.88.0",
    "url-parse": "^1.4.3"
  },
  "devDependencies": {
    "nodemon": "^1.17.5",
    "pre-commit": "^1.2.2"
  }
}
